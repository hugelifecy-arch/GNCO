{
  "name": "global-fund-architect",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "generate:investor-pages": "node ./scripts/generate-investor-pages.mjs",
    "build": "npm run generate:investor-pages && tsc -b && vite build",
    "guard:investor-claims": "node ./scripts/investor-claims-guard.mjs",
    "build:compliance": "npm run generate:investor-pages && npm run validate:jurisdictions && tsc -b && vite build && npm run guard:investor-claims",
    "preview": "vite preview",
    "test": "jest && npm run test:guard",
    "validate:jurisdictions": "node ./scripts/validate-jurisdictions.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.mjs --max-warnings=0",
    "format:check": "prettier --check \"scripts/**/*.mjs\" \"README.md\" \"../README.md\" \"../CONTRIBUTING.md\" \"../SECURITY.md\"",
    "test:guard": "node --test ./scripts/tests/claim-rules.test.mjs"
  },
  "dependencies": {
    "dagre": "^0.8.5",
    "lucide-react": "^0.454.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactflow": "^11.11.4",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "jest": "^29.7.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "ts-jest": "^29.4.6",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "eslint": "^9.20.1",
    "@eslint/js": "^9.20.0",
    "typescript-eslint": "^8.24.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "prettier": "^3.5.1",
    "globals": "^15.15.0"
  }
}